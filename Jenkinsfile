pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('CodeBuild') {
            steps {
                echo 'Building'
            }
        }
        stage('CodeDeploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('CodeRelease') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
